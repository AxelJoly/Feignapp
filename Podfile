# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Feignapp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
platform :ios, '11.0'

  use_frameworks!
  pod 'Alamofire', '~> 4.0'
  pod 'PopupDialog', '~> 0.7'
  pod 'SwiftyJSON'
  pod 'Firebase/Auth'
  pod 'SearchTextField'
 
  
  
  # Pods for Feignapp

  target 'FeignappTests' do

    inherit! :search_paths
    # Pods for testing
  end

  target 'FeignappUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Feignapp WatchKit App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '3.0'
  use_frameworks!
  pod 'Alamofire', '~> 4.0'
  pod 'SwiftyJSON'

  # Pods for Feignapp WatchKit App

end

target 'Feignapp WatchKit Extension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  platform :watchos, '3.0'
  use_frameworks!
  pod 'Alamofire', '~> 4.0'
  pod 'SwiftyJSON'
  pod 'EMTLoadingIndicator', '~> 4.0.0'
  # Pods for TrainApp WatchKit Extension

end
